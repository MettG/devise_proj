<%= button_to "Log Out", destroy_user_session_path, method: :delete %>
<h2>Welcome <%= current_user.user_name %></h2>
<hr />
<%= link_to "Add Task", new_task_path %>
<h3>Your Tasks</h3>
<ul>
  <% @tasks.each do |task| %>
    <li>
      <%=task.body%> <br/>
			<%="Start Date: #{task.start}"%> <br/>
			<%="Due Date: #{task.due}"%>
    </li>
  <% end %>
</ul>